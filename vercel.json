{
    "version": 2,
    "functions": {
        "api/index.php": {
            "runtime": "vercel-php@0.4.0"
        }
    },
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/api/index.php"
        },
        {
            "src": "/css/filament/(.*)",
            "dest": "/public/css/filament/"
        },
        {
            "src": "/js/filament/(.*)",
            "dest": "/public/js/filament/"
        },
        {
            "src": "/build/asset(.*)",
            "dest": "/public/build/asset/"
        }
    ],
    "env": {
        "APP_ENV": "production",
        "APP_DEBUG": "true",
        "APP_URL": "https://laravel-gsheet.vercel.app",
        "APP_KEY": "base64:OKWQ2iI3Y9HiiwcdQqdWj5zGPcaT7bD6zbQ3coWGcns=",
        "APP_CONFIG_CACHE": "/tmp/config.php",
        "APP_EVENTS_CACHE": "/tmp/events.php",
        "APP_PACKAGES_CACHE": "/tmp/packages.php",
        "APP_ROUTES_CACHE": "/tmp/routes.php",
        "APP_SERVICES_CACHE": "/tmp/services.php",
        "VIEW_COMPILED_PATH": "/tmp",
        "CACHE_DRIVER": "array",
        "LOG_CHANNEL": "stderr",
        "SESSION_DRIVER": "cookie",
        "DB_CONNECTION": "sqlite",
        "DB_DATABASE": "/tmp/database.sqlite"
    },
    "outputDirectory": "public",
    "rewrites": [
    { "source": "/favicon.ico", "destination": "https://juicy.vercel.app/favicon.ico" },
    { "source": "/(.*)", "destination": "/api/index.php" }
  ]
}